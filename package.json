{
  "name": "ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-dev": "vue-cli-service serve --config=dev --open",
    "serve-rel": "vue-cli-service serve --config=rel --open",
    "serve-online": "vue-cli-service serve --config=online --open",
    "build-dev": "vue-cli-service build --config=dev",
    "build-rel": "vue-cli-service build --config=rel",
    "build-online": "vue-cli-service build --config=online",
    "lint": "tslint -p ./ --format json --out reports/tsReport.json",
    "test:unit": "vue-cli-service test:unit",
    "inspect": "vue-cli-service inspect > webpackConfig.md",
    "stylelint": "stylelint src/**/*.scss -q -f json > reports/scssReport.json"
  },
  "dependencies": {
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios": "^0.18.0",
    "babel-plugin-component": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "element-ui": "^2.13.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.9.0",
    "stylelint-config-ydj": "^0.1.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "yargs": "^12.0.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "src/**/*.s+(a|c)ss": [
      "stylelint src/**/*.scss -q"
    ]
  }
}
